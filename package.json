{
  "name": "algebraic-fx",
  "version": "0.0.1",
  "description": "A tiny TEA-style runtime with explicit algebraic effects over a fast virtual DOM",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Boaz Blake <boazblake@gmail.com> (https://boazblake.github.io/identity/)",
  "license": "MIT",
  "keywords": [
    "algebraic-effects",
    "tea",
    "elm-architecture",
    "virtual-dom",
    "vdom",
    "functional-programming",
    "fp",
    "monad",
    "adt",
    "reactive",
    "ui-framework",
    "hyperscript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boazblake/algebraic-fx.git"
  },
  "bugs": {
    "url": "https://github.com/boazblake/algebraic-fx/issues"
  },
  "homepage": "https://github.com/boazblake/algebraic-fx#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core/*": "./dist/core/*.js",
    "./adt/*": "./dist/adt/*.js",
    "./helpers/*": "./dist/helpers/*.js"
  },
  "scripts": {
    "build": "tsc -p .",
    "dev": "esbuild src/index.ts --bundle --servedir=public --outfile=public/app.js --sourcemap",
    "serve": "serve public",
    "lint": "oxlint . && eslint . --ext .ts,.js",
    "lint:fix": "oxlint --fix . && eslint --fix . --ext .ts,.js",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build",
    "prepack": "npm run pack",
    "prepare": "npm run bench; npm run docs;",
    "docs": "typedoc",
    "test": "npx vitest --watch",
    "bench": "npm run build && node benchmarks/runner.js"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "esbuild": "^0.21.4",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "fast-check": "^3.18.0",
    "happy-dom": "^20.0.11",
    "oxlint": "^1.16.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.15",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.4.0",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
